generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Edge {
  id          String @id @default(uuid())
  created_at  String
  updated_at  String
  capacity    Int
  node1_alias String
  node2_alias String
}
